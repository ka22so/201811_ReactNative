{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "jsx-no-lamda": true,
        "jsx-boolean-value": false,
        "quotemark": [
            true,
            "single"
        ],
        "interface-over-type-literal": false,
        "member-access": false
    },
    "rulesDirectory": []
}